    xmlns:d="http://xamarin.com/schemas/2014/forms/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <!--  Dark  -->
    <Color x:Key="DarkPrimaryColor">#00ADEF</Color>
    <Color x:Key="DarkPrimaryLightColor">#89DFFF</Color>
    <Color x:Key="DarkPrimaryDarkColor">#00709A</Color>

    <Color x:Key="DarkFirstOtherColor">#404040</Color>
    <Color x:Key="DarkSecondOtherColor">#BFB995</Color>
    <Color x:Key="DarkTopIconForeground">#79E0DF</Color>

    <Color x:Key="DarkHyperLink">#567cd7</Color>
    <Color x:Key="DarkFacebookBlue">#4267B2</Color>
    <Color x:Key="DarkGoogleOrange">#da4453</Color>

    <Color x:Key="DarkBackgoundPageColor">#F3F3F3</Color>
