    <Color x:Key="DarkSearchBarBackgroundColor">White</Color>

    <Color x:Key="DarkSfTextInputErrColor">Red</Color>
    <Color x:Key="DarkSfTextInputFocusColor">Green</Color>

    <Color x:Key="DarkTabItemSelectedColor">#00ADEF</Color>
    <Color x:Key="DarkTabItemUnSelectedColor">#787878</Color>
