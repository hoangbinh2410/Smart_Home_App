    <x:Double x:Key="DarkTabItemNormalSize">30</x:Double>
    <!--  40x40  -->
    <x:Double x:Key="DarkTabItemClickedSize">40</x:Double>
    <FileImageSource x:Key="DarkHomePageTopImageSource" File="dark_top_bg.jpg" />
    <FileImageSource x:Key="DarkHomePageBotImageSource" File="dark_bot_bg.jpg" />

    <Color x:Key="DarkBackgoundFavouriteColor">White</Color>
    <Color x:Key="DarkBackgoundFavouriteGradientStartColor">White</Color>
    <Color x:Key="DarkBackgoundFavouriteGradientEndColor">Black</Color>

